<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>openTCS-2.7</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>openTCS-2.7</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>openTCS-HEAD</label>
                    <location>.</location>
                    <encoding>ISO-8859-15</encoding>
                </source-folder>
                <source-folder>
                    <label>src-base</label>
                    <type>java</type>
                    <location>src-base</location>
                    <encoding>ISO-8859-15</encoding>
                </source-folder>
                <source-folder>
                    <label>src-kernel</label>
                    <type>java</type>
                    <location>src-kernel</location>
                    <encoding>ISO-8859-15</encoding>
                </source-folder>
                <source-folder>
                    <label>src-plantoverview</label>
                    <type>java</type>
                    <location>src-plantoverview</location>
                    <encoding>ISO-8859-15</encoding>
                </source-folder>
                <source-folder>
                    <label>tests</label>
                    <type>java</type>
                    <location>tests</location>
                    <encoding>ISO-8859-15</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>nbproject/nbjdk.xml</script>
                    <target>buildall</target>
                </action>
                <action name="clean">
                    <script>nbproject/nbjdk.xml</script>
                    <target>cleanall</target>
                </action>
                <action name="javadoc">
                    <script>nbproject/nbjdk.xml</script>
                    <target>buildapidocs</target>
                </action>
                <action name="rebuild">
                    <script>nbproject/nbjdk.xml</script>
                    <target>cleanall</target>
                    <target>buildall</target>
                </action>
                <action name="compile.single">
                    <script>build.xml</script>
                    <target>compileselectedfiles</target>
                    <context>
                        <property>selectedfiles</property>
                        <folder>src</folder>
                        <pattern>.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="run.single">
                    <script>build.xml</script>
                    <target>runselectedfile</target>
                    <context>
                        <property>runclass</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="test.single">
                    <script>build.xml</script>
                    <target>runsingletest</target>
                    <context>
                        <property>runtests</property>
                        <folder>tests</folder>
                        <pattern>.java$</pattern>
                        <format>relative-path-noext</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="test">
                    <target>runtests</target>
                </action>
            </ide-actions>
            <export>
                <type>jar</type>
                <location>lib/openTCS-Base.jar</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>buildall</build-target>
            </export>
            <export>
                <type>jar</type>
                <location>lib/openTCS-Kernel.jar</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>buildall</build-target>
            </export>
            <export>
                <type>jar</type>
                <location>lib/openTCS-PlantOverview.jar</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>buildall</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src-base</label>
                        <location>src-base</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src-kernel</label>
                        <location>src-kernel</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src-plantoverview</label>
                        <location>src-plantoverview</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>tests</label>
                        <location>tests</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="test"/>
                </context-menu>
            </view>
            <subprojects/>
